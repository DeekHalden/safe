-var data = [{title:'pay less',className:'less',popup:'#top-form-popup'},{title:'pay more', className:'more',popup:'#small-thanks'}]
-var td = [{title:'Gas fee',cost:'$0'},{title:'Truck fee',cost:'$0'},{title:'Stairs fee',cost:'$0'},{title:'Two shrink wrap rolls',cost:'$0'},{title:'Two masking tape',cost:'$0'},{title:'Five plastic bags',cost:'$0'},{title:'Packing paper',cost:'$0'}]

each el, i in data
    +b.reason-item(class="reason-item--"+el.className)
        +b.img-wrapper
            +e.IMG(src="./images/logo.png").img
        +b.SPAN.cost-p-hour Pay $55 <br>per hour
        +b.TABLE.table
                each el, i in td
                    +b.TR.table-row
                        +e.TD.first(data-td=td[i].title)=el.title
                        +e.TD.last=el.cost

        +b.P.total-info total bill = 
            +e.SPAN.total $300
        +b.P.conclusion 
            +e.SPAN.styled $180 economy 
            +e.SPAN than Pure Moving


        +b.A(href=el.popup, class="badge--"+el.className).badge #{el.title}

.container#top-form-popup.zoom-anim-dialog.mfp-hide.dialog
    +b.FORM(action="").popup-form.clearfix#popup-form
        +e.H2.title REQUEST <br>A QUOTE NOW
        +b.first-step(data-step="1")#first-step
            +b.input-group
                +e.LABEL.label(for="popupfrom") FROM:
                +e.INPUT(type="text" id="popupfrom" placeholder="city of zip code" name="popupfrom" required).input.form-control
            +b.input-group
                +e.LABEL.label(for="popupto") TO:
                +e.INPUT(type="text" id="popupto" placeholder="city of zip code" name="popupto" required).input.form-control
            +b.input-group
                +e.LABEL.label(for="popupsize" ) HOME SIZE:
                +b.SELECT.select(id="popupsize" name="popupsize" required).input.form-control.form-control--select
                    +e.OPTION.el(data-index="0" value="") ---
                    +e.OPTION.el(data-index="1" value="1") Studio
                    +e.OPTION.el(data-index="2" value="2") 1 bedroom
                    +e.OPTION.el(data-index="3" value="3") 2 bedroom(small size,few items)
                    +e.OPTION.el(data-index="4" value="4") 2 bedroom(avg. size, avg. items)
                    +e.OPTION.el(data-index="5" value="5") 2 bedroom(large, many items)
                    +e.OPTION.el(data-index="6" value="6") 3 bedroom(avg. size, avg. items)
                    +e.OPTION.el(data-index="7" value="7") 3 bedroom(large, many items)
                    +e.OPTION.el(data-index="8" value="8") 4 bedroom(avg. size, avg. items)
                    +e.OPTION.el(data-index="9" value="") 4 bedroom(large, many items)
                    +e.OPTION.el(data-index="10" value="") Commercial(avg. size, avg. items)
                    +e.OPTION.el(data-index="11" value="last") Commercial(large, many items)
            +b.input-group.input-group--date
                +e.LABEL.label(for="popupdate") DATE:
                +e.INPUT(type="text"  placeholder="date" id="popupdate" name="popupdate" required readonly="true").input.form-control.input-date
        +b.second-step(data-step="2")#second-step
            +b.input-group
                +e.LABEL.label(for="popupname") YOUR NAME:
                +e.INPUT(type="text" required id="popupname" placeholder="your name" name="popupname").input.form-control
            +b.input-group
                +e.LABEL.label(for="popuphear") HOW DID YOU HEAR ABOUT US:
                +b.SELECT.select(id="popuphear" required name="popuphear").input.form-control.form-control--select
                    +e.OPTION.el(data-index="0" value="") ---
                    +e.OPTION.el(data-index="1" value="1") Studio
                    +e.OPTION.el(data-index="2" value="2") 1 bedroom
                    +e.OPTION.el(data-index="3" value="3") 2 bedroom(small size,few items)
                    +e.OPTION.el(data-index="4" value="4") 2 bedroom(avg. size, avg. items)
                    +e.OPTION.el(data-index="5" value="5") 2 bedroom(large, many items)
                    +e.OPTION.el(data-index="6" value="6") 3 bedroom(avg. size, avg. items)
                    +e.OPTION.el(data-index="7" value="7") 3 bedroom(large, many items)
                    +e.OPTION.el(data-index="8" value="8") 4 bedroom(avg. size, avg. items)
                    +e.OPTION.el(data-index="9" value="") 4 bedroom(large, many items)
                    +e.OPTION.el(data-index="10" value="") Commercial(avg. size, avg. items)
                    +e.OPTION.el(data-index="11" value="last") Commercial(large, many items)
            +b.input-group
                +e.LABEL.label(for="popupphone" ) PHONE:
                +e.INPUT(type="text" id="popupphone" required placeholder="your phone" name="popupphone").input.form-control
            +b.input-group
                +e.LABEL.label(for="popupemail") EMAIL :
                +e.INPUT(type="text" required  placeholder="your email" id="popupemail"  name="popupemail").input.form-control 
            +b.input-group.input-group--button
                +b.INPUT(type="submit" value="get started").action-btn.action-btn--submit
    +b.dialog.zoom-anim-dialog.mfp-hide
        +e.H1.el Thank you! Our manager will contact you as soon as possible!

+b.dialog.zoom-anim-dialog.mfp-hide#small-thanks
    +e.H1.el Thank you! Our manager will contact you as soon as possible!
    